{
  "name": "awesome-app",
  "version": "0.0.1",
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:jafar-jabr/react-native-typescript-boilerplate.git"
  },
  "keywords": [
    "react-native-typescript-boilerplate",
    "boilerplate",
    "react-native-typescript",
    "react-native",
    "typescript",
    "redux",
    "redux-saga"
  ],
  "description": "React Native Boilerplate, TypeScript with @react-native-navigation and redux-saga",
  "author": {
    "name": "Jafar Jabr",
    "email": "jafar.jabr.dev@gmail.com",
    "url": "https://github.com/jafar-jabr"
  },
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start  --reset-cache",
    "test": "jest",
    "lint": "eslint . -c .eslintrc.json --ext js,jsx,ts,tsx"
  },
  "dependencies": {
    "@react-native-community/async-storage": "1.12.1",
    "@react-native-community/audio-toolkit": "2.0.3",
    "@react-native-community/masked-view": "0.1.10",
    "@react-native-community/netinfo": "5.9.7",
    "@react-navigation/drawer": "5.9.3",
    "@react-navigation/native": "5.7.6",
    "@react-navigation/stack": "5.9.3",
    "@redux-saga/core": "1.1.3",
    "appcenter-analytics": "3.1.2",
    "axios": "0.21.0",
    "date-dealer": "2.2.1",
    "i18n-js": "3.8.0",
    "lodash": "4.17.20",
    "prop-types": "15.7.2",
    "react": "17.0.1",
    "react-native": "0.63.3",
    "react-native-camera": "3.40.0",
    "react-native-config": "1.3.3",
    "react-native-elements": "2.3.2",
    "react-native-fs": "2.16.6",
    "react-native-gesture-handler": "1.8.0",
    "react-native-image-picker": "2.3.4",
    "react-native-permissions": "2.2.2",
    "react-native-reanimated": "1.13.1",
    "react-native-safe-area-context": "3.1.8",
    "react-native-safe-area-view": "1.1.1",
    "react-native-screens": "2.11.0",
    "react-native-vector-icons": "7.1.0",
    "react-redux": "7.2.1",
    "redux": "4.0.5",
    "redux-saga": "1.1.3"
  },
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@babel/runtime": "7.12.1",
    "@react-native-community/eslint-config": "2.0.0",
    "@types/jest": "26.0.15",
    "@types/react": "16.9.53",
    "@types/react-native": "0.63.30",
    "@types/react-test-renderer": "16.9.3",
    "@typescript-eslint/eslint-plugin": "4.5.0",
    "@typescript-eslint/parser": "4.5.0",
    "babel-jest": "26.6.1",
    "eslint": "7.11.0",
    "eslint-config-airbnb": "18.2.0",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-config-prettier": "6.14.0",
    "eslint-import-resolver-webpack": "0.13.0",
    "eslint-plugin-detox": "1.0.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-react-redux": "3.3.0",
    "jest": "26.6.1",
    "metro-react-native-babel-preset": "0.63.0",
    "react-native-typescript-transformer": "1.2.13",
    "react-test-renderer": "17.0.1",
    "ts-jest": "26.4.2",
    "typescript": "4.0.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/"
    ],
    "cacheDirectory": ".jest/cache"
  }
}
