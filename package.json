    "@react-navigation/stack": "^5.4.0",
    "@redux-saga/core": "^1.1.3",
    "appcenter-analytics": "^3.0.2",
    "axios": "^0.19.2",
    "date-dealer": "^2.1.4",
    "i18n-js": "^3.5.1",
    "lodash": "^4.17.15",
    "prop-types": "^15.7.2",
    "react": "16.11.0",
    "react-native": "0.62.2",
    "react-native-camera": "^3.26.0",
